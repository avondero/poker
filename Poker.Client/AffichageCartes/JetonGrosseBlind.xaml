<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Poker.Client.JetonGrosseBlind"
	x:Name="UserControl"
	d:DesignWidth="20" d:DesignHeight="20">

	<Grid x:Name="LayoutRoot">
		<Ellipse Margin="0,0,0,0" Fill="#FFFEFD03" Stroke="#FF000000" Width="20" Height="20"/>
		<TextBlock d:IsStaticText="True" FontFamily="./..\Fonts\#Verdana" FontSize="8" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" Text="BB"><TextBlock.RenderTransform>
				<TransformGroup>
					<ScaleTransform ScaleX="1" ScaleY="1"/>
					<SkewTransform AngleX="0" AngleY="0"/>
					<RotateTransform Angle="-7"/>
					<TranslateTransform X="0" Y="0"/>
				</TransformGroup>
			</TextBlock.RenderTransform></TextBlock>
	</Grid>
</UserControl>